  "repository": {
    "type": "git",
    "url": "https://github.com/fouad-j/init-project-angular.git"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "prebuild": "npm clean && mkdir -p ./dist/assets",
    "build:css": "lessc ./src/app.less ./build/styles.css && lessc --clean-css ./build/styles.css ./build/styles.min.css",
    "build:assets": "node ./tasks-build/assets",
    "build:templates": "node ./node_modules/npm-html2js/bin/npm-html2js -i \"./src/**/*[^index].html\" -o \"./build/tmp/template.js\" -m \"app\" --minify",
    "build:js": "node ./tasks-build/javascript",
    "verify:js": "eslint src/**/*.js",
    "verify:less": "lessc -l ./src/app.less",
    "verify": "npm-run-all --parallel verify:*",
    "watch:css": "onchange ./src/**/*.less -v -- npm run build:css",
    "watch": "npm-run-all --parallel watch:*",
    "test": "karma start --no-auto-watch",
    "test:w": "karma start --auto-watch"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "dependencies": {
    "angular": "^1.5.7",
    "angular-ui-router": "^0.3.1",
    "bootstrap": "^3.3.6",
    "jquery": "^2.0.0",
    "font-awesome": "^4.6.3"
  },
  "devDependencies": {
    "add-stream": "^1.0.0",
    "angular": "^1.5.8",
    "angular-mocks": "^1.5.8",
    "babel-preset-es2015": "^6.13.2",
    "browser-sync": "^2.13.0",
    "cpx": "^1.5.0",
    "del": "^2.2.1",
    "eslint": "^3.0.0",
    "eslint-config-google": "^0.6.0",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-jasmine": "^1.8.1",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-standard": "^1.3.2",
    "express": "^4.14.0",
    "gulp": "^3.9.1",
    "gulp-angular-templatecache": "^2.0.0",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.0",
    "gulp-cssmin": "^0.1.7",
    "gulp-develop-server": "^0.5.2",
    "gulp-eslint": "^2.0.0",
    "gulp-htmlmin": "^2.0.0",
    "gulp-less": "^3.1.0",
    "gulp-ng-annotate": "^2.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.5.4",
    "jasmine": "^2.4.1",
    "karma": "^0.13.22",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.4",
    "karma-ng-html2js-preprocessor": "^1.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "less": "^2.7.1",
    "less-plugin-clean-css": "^1.5.1",
    "lodash": "^4.15.0",
    "ng-html2js": "^3.0.0",
    "npm-html2js": "^0.1.8",
    "npm-run-all": "^3.1.0",
    "onchange": "^3.0.2",
    "phantomjs-prebuilt": "^2.1.7",
    "pre-commit": "^1.1.3",
    "rimraf": "^2.5.4",
    "run-sequence": "^1.2.2"
  }
}
